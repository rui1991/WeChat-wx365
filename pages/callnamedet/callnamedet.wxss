.content{
  background-color: #ffffff;
}

.content .title, .content .item{
  display: flex;
  align-items: center;
  height: 80rpx;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #f1f1f1;
  color: #686868;
  font-size: 28rpx;
}
.content .num {
  width: 80rpx;
}
.content .date, .content .state{
  flex-grow: 1;
  text-align: center;
}